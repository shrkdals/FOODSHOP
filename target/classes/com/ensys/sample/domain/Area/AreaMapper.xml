    </update>

    <update id="FS_AREA_SALES_PERSON_CRUD" parameterType="hashmap" statementType="CALLABLE">
      { CALL UP_FS_AREA_SALES_PERSON_CRUD(#{COMPANY_CD},#{CONTROL_AREA_CD} ,#{SALES_PERSON_ID} ,#{SALES_PERSON_LV} ,#{COMT_RATE} ,#{PARENT_SALES_PERSON} ,#{SORT} ,#{LOGIN_ID} ,#{CRUD_TYPE}) }
